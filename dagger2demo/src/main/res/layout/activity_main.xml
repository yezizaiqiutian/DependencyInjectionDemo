<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_001"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="无参数的注入" />

    <Button
        android:id="@+id/btn_002"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="有参数的注入" />

    <Button
        android:id="@+id/btn_003"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="抽象参数的注入" />

    <Button
        android:id="@+id/btn_004"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-抽象参数不同实现的注入" />

    <Button
        android:id="@+id/btn_005"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="三方参数的注入" />

    <Button
        android:id="@+id/btn_005_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="+module传参" />

    <Button
        android:id="@+id/btn_006"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="有参数的三方参数的注入" />

    <Button
        android:id="@+id/btn_007"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="有参数的三方参数的注入(测试单例)" />

    <Button
        android:id="@+id/btn_008"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Context参数的注入" />

    <Button
        android:id="@+id/btn_009"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ViewModel参数的注入" />

    <Button
        android:id="@+id/btn_0010"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ViewModel参数的注入(正常获取VM)" />

</LinearLayout>